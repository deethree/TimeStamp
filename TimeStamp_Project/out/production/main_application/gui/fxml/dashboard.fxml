<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane id="app" fx:id="borderpane" maxHeight="0.0" maxWidth="0.0" minHeight="0.0" minWidth="0.0" prefHeight="0.0"
            prefWidth="0.0" stylesheets="@../css/ui_view.css" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.CTR_Dashboard">
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
   <left>
      <HBox fx:id="left_menu_hbox" alignment="CENTER_RIGHT" BorderPane.alignment="CENTER">
         <fx:include fx:id="em_Menu" source="main_menu.fxml"/>

      </HBox>
   </left>
   <bottom>
      <VBox BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
         </BorderPane.margin>
         <HBox>
            <VBox.margin>
               <Insets/>
            </VBox.margin>
            <Label fx:id="label_build"/>
         </HBox>
      </VBox>
   </bottom>
   <center>
      <VBox alignment="TOP_RIGHT" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
         </BorderPane.margin>
         <ScrollPane minHeight="80.0">
            <content>
               <VBox fx:id="vbox_projList"/>
            </content>
         </ScrollPane>

      </VBox>
   </center>
</BorderPane>
